        "@types/jest": "^29.5.2",
        "@types/node": "^20.2.5",
        "@types/redis": "^4.0.11",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "babel-jest": "^29.5.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^35.0.0",
